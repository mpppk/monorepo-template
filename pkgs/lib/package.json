    "dotenv-cli": "^7.2.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^9.0.0",
    "npm-run-all2": "^7.0.0",
    "prettier": "^2.8.7",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.2",
    "vitest": "^2.0.0",
    "zod": "^3.21.4"
  },
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
